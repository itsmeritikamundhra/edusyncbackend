{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "<YOUR_DB_CONNECTION_STRING>"
  },
  "JwtSettings": {
    "SecretKey": "<YOUR_JWT_SECRET>",
    "Issuer": "EduSyncAPI",
    "Audience": "EduSyncClient",
    "ExpiryMinutes": 60
  }
}
